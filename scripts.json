{
    "$schema": "https://deno.land/x/denon/schema.json",
    "scripts": {
        "start": "deno run --allow-net --allow-env --allow-read --unstable --import-map=import_map.json src/server.ts",
        "dev": "deno run --allow-net --allow-env --allow-read --unstable --import-map=import_map.json --no-check src/server.ts"
    },
    "watcher": {
      "match": ["deno.json", "src/**/*.ts", "src/**/*.tsx", "public/windi.css"]
    }
}